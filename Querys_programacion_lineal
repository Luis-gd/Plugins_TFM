Get_aeropuertos_salidas: 
MATCH (c:Country)-[]-(entrada:Airport)
    -[]->(:AirportOperationDay)<-[]-(f:FLIGHT)<-[]-(:AirportOperationDay)<-[]-(salida:Airport)  
    WHERE c.countryName= 'Spain'
    AND not (f.flightIfinal =  '') 
    AND not (salida.iata = '')
    AND date('2020-09-24') <= date(f.dateOfDeparture) <= date('2020-09-24')
    RETURN DISTINCT  salida.iata, salida.airportName  ORDER BY salida.iata
    
Get_aeropuertos_entradas:
MATCH (c:Country)-[]-(entrada:Airport)
    -[]->(:AirportOperationDay)<-[]-(f:FLIGHT)<-[]-(:AirportOperationDay)<-[]-(salida:Airport)  
    WHERE c.countryName= 'Spain'
    AND not (f.flightIfinal =  '') 
    AND not (entrada.iata = '')
    AND date('2020-09-24') <= date(f.dateOfDeparture) <= date('2020-09-24')
    RETURN DISTINCT  entrada.iata, entrada.airportName  ORDER BY entrada.iata

Get_sir:
MATCH (c:Country)-[]-(entrada:Airport)
    -[]->(:AirportOperationDay)<-[]-(f:FLIGHT)<-[]-(:AirportOperationDay)<-[]-(salida:Airport)
    WHERE c.countryName= 'Spain'
    AND not (f.flightIfinal = '')
    AND not (salida.iata = '')
    AND not (entrada.iata = '')
    AND date('2020-09-24') <= date(f.dateOfDeparture) <= date('2020-09-24')
    RETURN f.flightIfinal AS sir, entrada.iata AS aeropuertoLlegada, salida.iata AS aeropuertoSalida,
    entrada.airportName, salida.airportName ORDER BY  entrada.iata, salida.iata

Get_pasajeros_por_vuelo:
MATCH (c:Country)-[]-(entrada:Airport)
    -[]->(:AirportOperationDay)<-[]-(f:FLIGHT)<-[]-(:AirportOperationDay)<-[]-(salida:Airport)  
    WHERE c.countryName= 'Spain'
    AND not (f.flightIfinal =  '')
    AND not (salida.iata = '')
    AND not (entrada.iata = '')
    AND date('2020-09-24') <= date(f.dateOfDeparture) <= date('2020-09-24')
    RETURN round(f.occupancyPercentage * f.seatsCapacity/100), entrada.iata, salida.iata, entrada.airportName,
    salida.airportName ORDER BY  entrada.iata, salida.iata

Get_pasajeros_por_vuelo_y_companyias:
MATCH (c:Country)-[]-(entrada:Airport)
    -[]->(:AirportOperationDay)<-[]-(f:FLIGHT)<-[]-(:AirportOperationDay)<-[]-(salida:Airport)  
    WHERE c.countryName= 'Spain'
    AND not (f.flightIfinal =  '') 
    AND not (salida.iata = '')
    AND not (entrada.iata = '')
    AND date('2020-09-24') <= date(f.dateOfDeparture) <= date('2020-09-24')
    RETURN round(f.occupancyPercentage * f.seatsCapacity/100), f.operator, entrada.iata, salida.iata,
    entrada.airportName, salida.airportName ORDER BY  entrada.iata, salida.iata

Get_companyias:    
MATCH (c:Country)-[]-(entrada:Airport)
    -[]->(:AirportOperationDay)<-[]-(f:FLIGHT)<-[]-(:AirportOperationDay)<-[]-(salida:Airport)
    WHERE c.countryName= 'Spain'
    AND not (f.flightIfinal = '')
    AND not (salida.iata = '')
    AND not (entrada.iata = '')
    AND date('2020-09-24') <= date(f.dateOfDeparture) <= date('2020-09-24')
    RETURN DISTINCT f.operator ORDER BY f.operator

//(Devuelve el nombre de la entrada y salida de una conexion, los vuelos totales que tiene el aeropuerto de salida,
//la cantidad de esos vuelos que vienen a espaÃ±a y la conectividad del aeropuerto de salida)
Get_conectividad_por_aeropuerto:
MATCH (salida:Airport)-[]-(:AirportOperationDay)-[]->(vuelosSalientes:FLIGHT)
    WHERE date('2020-09-24') <= date(vuelosSalientes.dateOfDeparture) <= date('2020-09-24')
    AND salida.connectivity IS NOT NULL
    AND not (salida.iata = '')
    WITH salida, COUNT(vuelosSalientes) AS totalVuelosSalientes, salida.connectivity AS conectividadSaliente
    MATCH (salida)-[]-(:AirportOperationDay)-[]->(vuelosEntrantes:FLIGHT)-[]->(:AirportOperationDay)-[]-(llegada:Airport)-[]-(c:Country)
    WHERE date('2020-09-24') <= date(vuelosEntrantes.dateOfDeparture) <= date('2020-09-24')
    AND c.countryName = 'Spain'
    AND not (llegada.iata = '')
    RETURN llegada.iata, salida.iata, COUNT(vuelosEntrantes) AS totalVuelosEntrantes, totalVuelosSalientes, conectividadSaliente, llegada.airportName, salida.airportName
    ORDER BY llegada.iata, salida.iata

Get_dinero_por_vuelo:
MATCH (c:Country)-[]-(entrada:Airport)
    -[]->(:AirportOperationDay)<-[]-(f:FLIGHT)<-[]-(:AirportOperationDay)<-[]-(salida:Airport)
    WHERE c.countryName= 'Spain'
    AND not (f.flightIfinal =  '')
    AND not (salida.iata = '')
    AND not (entrada.iata = '')
    AND date('2020-09-24') <= date(f.dateOfDeparture) <= date('2020-09-24')
    RETURN f.incomeFromTurism, entrada.iata, salida.iata, entrada.airportName, salida.airportName
    ORDER BY  entrada.iata, salida.iata
